    "serve": "vite preview",
    "test": "vitest run",
    "prepare": "husky",
    "release": "semantic-release --no-ci",
    "release:dry": "semantic-release --dry-run"
  },
  "peerDependencies": {
    "@tanstack/react-router": "^1.157.18",
